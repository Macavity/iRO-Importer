{
  "name": "iRO Import Extension",
  "version": "0.1.9",
  "manifest_version": 2,
  "description": "This extension let's you import profiles into iRO.",
  "homepage_url": "http://www.heads2hunt.de",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "bg/background.html"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "iRO Importer",
    "default_popup": "views/browser_action.html"
  },
  "permissions": [
    "notifications",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "inject/inject.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}